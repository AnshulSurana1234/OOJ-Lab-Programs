#include <stdio.h>

int main() 
{
    int i,j,x,n;
    x=1;
    printf("Enter the value of n\n");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        //printf blanks
        for(j=1;j<=i;j++)
        
        //printf number
        
        {
            printf("%d",x);
            x++;
        }
        
        printf("\n");
    }
}
