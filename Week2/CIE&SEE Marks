
#include <stdio.h>
int main()
{
    float CIEMarks;
    float SEEMarks;
    float total;
    printf("Enter CIEMarks");
    scanf("%f",&CIEMarks);
    printf("Enter SEEMarks");
    scanf("%f",&SEEMarks);
    do
    {
    total=(CIEMarks+(SEEMarks/2));
    if(total>=90)
    printf("S");
    else if(total>=80&&total<=90)
    printf("A");
    else if(total>=70&&total<=80)
    printf("B");
    else if(total>=60&&total<=70)
    printf("C");
    else if(total>=50&&total<=60)
    printf("D");
    else if(total>=40&&total<=50)
    printf("E");
    else 
    printf("FAIL");
    }
    while(CIEMarks<=50&&SEEMarks<=100);
    return total;
}
